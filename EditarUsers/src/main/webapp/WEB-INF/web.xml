<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>EditarUsers</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>LoginUser</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.LoginUser</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>UpdateUser</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.UpdateUser</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>RemoveUser</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.RemoveUser</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>InsertUser</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.InsertUser</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>LoginUser</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UpdateUser</servlet-name>
		<url-pattern>/UpdateUser</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RemoveUser</servlet-name>
		<url-pattern>/RemoveUser</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>InsertUser</servlet-name>
		<url-pattern>/InsertUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ListaPagos</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.ListadoPagos</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>UpdatePayment</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.UpdatePayment</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>RemovePayments</servlet-name>
		<servlet-class>org.iesalixar.servidor.controller.RemovePayments</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ListaPagos</servlet-name>
		<url-pattern>/ListaPagos</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UpdatePayment</servlet-name>
		<url-pattern>/UpdatePayment</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RemovePayments</servlet-name>
		<url-pattern>/RemovePayments</url-pattern>
	</servlet-mapping>
  
</web-app>